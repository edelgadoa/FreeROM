mesh Th=square(50,50,flags=3); 

//Esta funci√≥n lo que hace es refinar la malla en los bordes
func fx = 0.5*(1 + (tanh(2*(2*x-1)))/(tanh(2)));
func fy = 0.5*(1 + (tanh(2*(2*y-1)))/(tanh(2)));
 Th = movemesh(Th,[fx,fy]);
